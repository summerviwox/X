<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.summer.app.BlankFragment"
        android:label="blank_fragment"
        tools:layout="@layout/blank_fragment">
        <action
            android:id="@+id/action_blankFragment_to_blankFragment2"
            app:destination="@id/blankFragment2"
            app:enterAnim="@anim/h_fragment_enter"
            app:exitAnim="@anim/h_fragment_exit"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/h_fragment_enter"
            app:popExitAnim="@anim/h_fragment_exit"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.summer.app.BlankFragment2"
        android:label="fragment_blank2"
        tools:layout="@layout/fragment_blank2" />
</navigation>